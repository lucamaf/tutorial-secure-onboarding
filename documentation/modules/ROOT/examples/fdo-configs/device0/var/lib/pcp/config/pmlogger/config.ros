log mandatory on default {
  hinv.ncpu
  mem.physmem
  mem.util.available
  disk.dev.total
  kernel.all.cpu.idle
  kernel.all.pressure.cpu.some.avg
  kernel.all.pressure.io.full.avg
  kernel.all.pressure.io.some.avg
  kernel.all.pressure.memory.full.avg
  kernel.all.pressure.memory.some.avg
}
[access]
disallow .* : all;
disallow :* : all;
allow local:* : enquire;
